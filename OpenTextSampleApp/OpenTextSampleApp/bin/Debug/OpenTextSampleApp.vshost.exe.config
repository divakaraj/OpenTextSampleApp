<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <appSettings>
    <add key="FolderLocation" value="D:\OpenText\NetworkFolder"/>
    <add key="ErrorFolderLocation" value="D:\OpenText\ErrorFolder"/>
    <add key="ArchiveFolderLocation" value="D:\OpenText\ArchiveFolder"/>
    
    <add key="WaitTime" value="60"/>
    <add key="XSDFolder" value="..\..\Control.xsd"/>

    <add key="EmailToBeSent" value="N"/>
    <add key="DBFillingEnabled" value="Y"/>

    <!-- Email config section begins-->
    <add key="Mail.ServerName" value="mail.opentext.com"/>
    <add key="Mail.DefaultSenderMail" value="no-reply@opentext.com"/>
    <add key="Mail.IsAuthenticationRequired" value="N"/>
    <add key="Mail.UserName" value=""/>
    <add key="Mail.Password" value=""/>
    <add key="Mail.Port" value="587"/>
    <!-- Email config section ends-->

  </appSettings>
  <connectionStrings>
    <add name="ConnectionString" connectionString="Data Source=.;User ID=OpenTextSampleUser;password=password-1;Initial Catalog=OpenTextSampleDB;" providerName="System.Data.SqlClient" />
  </connectionStrings>
</configuration>